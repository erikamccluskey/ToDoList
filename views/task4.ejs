<div id="<%= _id %>" >
<div class="input-group mb-3">
 
    <div class="input-group-prepend">
      <div class="input-group-text">
        <form method="POST" action='/unfinish'>
        <input type="hidden" name="_id" value="<%=_id%>" >
        <input type="hidden" name="username" value="<%= username %>">
        <input onclick="{this.form.submit()}" type="checkbox" aria-label="Checkbox for following text input" checked>
       </form> 
      </div>
    </div>
    
    <input id="disabledInput" placeholder="<%= taskName %>" style="text-decoration: line-through;" type="text" class="form-control" aria-label="Text input with checkbox" disabled>
  </div>
</div>